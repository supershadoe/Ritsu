{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "es2022",
    "lib": ["es2022"],
    "module": "es2022",
    "rootDir": "./src",
    "moduleResolution": "node16",
    "types": ["@cloudflare/workers-types"],
    "allowJs": false,
    "noEmit": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  }
}
