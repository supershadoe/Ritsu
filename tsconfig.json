{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "module": "NodeNext",
    "rootDir": "./src",
    "moduleResolution": "NodeNext",
    "types": ["@cloudflare/workers-types"],
    "allowJs": false,
    "noEmit": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  }
}
